#include <stdio.h>
#include <ctype.h>
#include <string.h>

int main(){
    char p[100],k[100];
    gets(p); gets(k);
    int j=0;
    for(int i=0;p[i];i++){
        if(isalpha(p[i])){
            p[i]=(p[i]-'a'+k[j]-'a')%26+'a';
            j=(j+1)%strlen(k);
        }
    }
    printf("%s",p);
    return 0;
}
