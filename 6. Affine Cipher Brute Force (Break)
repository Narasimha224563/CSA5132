#include <stdio.h>

int gcd(int a,int b){ return b?gcd(b,a%b):a; }

int main(){
    char c[100];
    gets(c);

    for(int a=1;a<26;a++){
        if(gcd(a,26)!=1) continue;
        for(int b=0;b<26;b++){
            printf("a=%d b=%d : ",a,b);
            for(int i=0;c[i];i++)
                printf("%c",(a*(c[i]-'A')+b)%26+'A');
            printf("\n");
        }
    }
}
